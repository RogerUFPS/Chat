package model;

public class Message {
    private String message;
    private User sender;
    private int order;

    public void setMessage(String m){
        this.message = m;
    }

    public String getMessage(){
        return this.message;
    }

    public void setSender(User user){
        this.sender = user;
    }

    public User getSender(){
        return this.sender;
    }

    public int getOrder(){
        return this.order;
    }

    public void setOrder(int order){
        this.order = order;
    }

}
